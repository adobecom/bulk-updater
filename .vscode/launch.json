{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "name": "Bulk Update Example",
      "request": "launch",
      "command": "npm run bulk-update migration-example",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node-terminal",
      "name": "Bulk Update Custom Example",
      "request": "launch",
      "command": "node migration-example/custom-migration.js",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node-terminal",
      "name": "Blog Migration",
      "request": "launch",
      "command": "npm run migration:blog",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node-terminal",
      "name": "Test",
      "request": "launch",
      "command": "npm run test",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node-terminal",
      "name": "Test File",
      "request": "launch",
      "command": "npm run test ${file}",
      "cwd": "${workspaceFolder}"
    }
  ]
}
